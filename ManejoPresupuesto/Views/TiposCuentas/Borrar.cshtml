
@model TipoCuenta
@{
    ViewData["Title"] = "Borrar Tipo de Cuenta";
}

<h3 class=" mb-3">¿Seguro que quiere borrar @Model.Nombre ?</h3>

<form asp-action="BorrarTipoCuenta" asp-controller="TiposCuentas">
    <input type="hidden" asp-for="Id" />
    <button type="submit" class="btn btn-danger">Borrar</button>
    <a class=" btn btn-secondary" asp-action="Index">Cancelar</a>
</form>
